		"docker-compose.yml",
		"docker-compose.user.yml",
	],
	"service": "app",
	"workspaceFolder": "/workspace",
	"extensions": [
		"mblode.twig-language-2",
		"sibiraj-s.vscode-scss-formatter",
		"esbenp.prettier-vscode",
